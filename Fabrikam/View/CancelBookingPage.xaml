<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Fabrikam.View.CancelBookingPage">
  <StackLayout Padding="20">
    <Label Text="Enter your contact number:"></Label>
    <Entry x:Name="PhoneEntry"></Entry>
    <Button Text="Cancel your booking" VerticalOptions="Center" HorizontalOptions="Center" Clicked="OnCancelBookingButtonClicked"></Button>
  </StackLayout>
</ContentPage>